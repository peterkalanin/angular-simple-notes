{
  "name": "angular-notes",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "server:start": "node ./server/app.js",
    "server:serve": "nodemon --watch server ./server/app.js",
    "app:serve": "ng build --configuration=development --aot=false --progress=true --watch --output-path=dist --delete-output-path=false",
    "app:build": "ng build --configuration=production --aot=true --progress=true --output-path=dist --delete-output-path=true",
    "app:test": "ng test",
    "heroku-postbuild": "npm run app:build",
    "start": "npm run server:start"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.0.1",
    "@angular/cdk": "^13.1.1",
    "@angular/common": "^13.0.1",
    "@angular/compiler": "^13.0.1",
    "@angular/core": "^13.0.1",
    "@angular/forms": "^13.0.1",
    "@angular/platform-browser": "^13.0.1",
    "@angular/platform-browser-dynamic": "^13.0.1",
    "@angular/router": "^13.0.1",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "body-parser": "^1.19.1",
    "express": "^4.17.2",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.4",
    "@angular/cli": "~13.0.4",
    "@angular/compiler-cli": "^13.0.1",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "autoprefixer": "^10.4.1",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-coverage": "~2.0.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "postcss": "^8.3.11",
    "tailwindcss": "^3.0.8",
    "typescript": "~4.4.3"
  }
